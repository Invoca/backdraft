{
  "name": "backdraft-app",
  "version": "1.3.2",
  "description": "Plugin-based UI application framework built on top of and extending Backbone to create single page apps",
  "files": [
    "src/",
    "vendor/"
  ],
  "scripts": {
    "eslint": "eslint-git-changes --jsx",
    "setup": "npm install -g grunt-cli",
    "specs": "yarn run eslint; karma start --single-run",
    "specs_without_eslint": "karma start --single-run",
    "dev": "yarn run eslint; karma start",
    "examples": "grunt examples",
    "postversion": "git push --tags && yarn publish --new-version $npm_package_version && git push && echo \"Successfully released version $npm_package_version!\""
  },
  "author": "Invoca",
  "license": "MIT",
  "repository": "https://github.com/invoca/backdraft/",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.3.0",
    "backbone": "1.1.2",
    "bootstrap": "4.0.0",
    "datatables.net": "1.10.9",
    "eslint": "^4.19.1",
    "eslint-git-changes": "^2.0.1",
    "express": "^4.16.2",
    "grunt": "~0.4.4",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-webpack": "^3.0.2",
    "jasmine-core": "^2.99.1",
    "jquery": "jquery/jquery#1.10.2",
    "jquery-deparam": "0.4.2",
    "karma": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^2.0.9",
    "morgan": "^1.9.0",
    "popper.js": "1.12.9",
    "serve-index": "^1.9.1",
    "style-guide": "Invoca/style-guide",
    "underscore": "1.8.3",
    "webpack": "^3.10.0"
  },
  "peerDependencies": {
    "backbone": "^1.1.2",
    "bootstrap": "^4.0.0",
    "jquery": "^1.10.2",
    "jquery-deparam": "^0.4.2",
    "popper.js": "^1.12.9",
    "underscore": "^1.8.3"
  }
}
